{
  "functions": {
    "api/index.py": {
      "runtime": "python3.11"
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/index.py"
    },
    {
      "source": "/",
      "destination": "/ui/index.html"
    },
    {
      "source": "/(?!ui/)(.*)",
      "destination": "/ui/$1"
    }
  ]
}
